#include <iostream>
using namespace std;

int main(){

    int number_1, number_2, number_3, arsents_number;

    number_1 = 6;
    number_2 = 24;
    number_3 = 400;
    arsents_number = 8 * (2* (number_1 + number_3) / (5 - (number_2 * number_1)));

    cout<< "Arsent's number is " <<arsents_number <<endl;


return 0;
}
